<stylesheet>
	<initialize>
		<alias id="style.normal" value="style.Normal"/>
	</initialize>

	<paraStyle name="h1"
	fontName="Arial-Bold"
	fontSize="12"
	leading="14.4"
	spaceBefore = "0.5 cm"
	/>

	<paraStyle name="h2"
	parent="h1"
	fontSize="11"
	leading="12.1"
	spaceBefore = "0.5 cm"
	/>

	<paraStyle name="normal"
	fontName="Arial"
	fontSize="10"
	leading="12"
	spaceBefore="12"
	/>

	<blockTableStyle id="basic" spaceBefore="0" spaceAfter="0" >
		<blockFont name="Arial" size="10" leading="12"/>
		<lineStyle
			colorName="black"
			kind="GRID"
			thickness="1"
			space="12"
			/>
	</blockTableStyle>
</stylesheet>


